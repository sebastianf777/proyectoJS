$.ajax({
	url: 'https://fakestoreapi.com/products',
	method: "GET",
	dataType: 'json',
	success: function (data) {
		console.log(data);
		generarEnemigo(data) // data.results = arreglo de usuarios
	},
	error: function (xhr, status, error) {
		console.log(error);
	}
});


